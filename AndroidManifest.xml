<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="ws.munday.barcamptampa"
      android:versionCode="7"
      android:versionName="1.06">
    <uses-sdk android:minSdkVersion="4" />

    <application android:icon="@drawable/ic_launcher" android:label="@string/app_name">
        <activity android:name=".BarcampTampaActivity" android:theme="@style/Theme.Main" android:label="@string/app_name" android:configChanges="orientation|keyboardHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
